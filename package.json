{
  "name": "carrier-service-backend",
  "version": "1.0.0",
  "description": "Correos de Chile - Shopify Carrier Service",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "SET NODE_ENV=staging ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "NODE_ENV=staging nodemon",
    "start:debug": "NODE_ENV=staging nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && npm run build",
    "start:prod": "SET NODE_ENV=production node dist/main.js",
    "start:pm2": "npx pm2 start pm2.yaml",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migrate": "SET NODE_ENV=production&& ts-node ./node_modules/typeorm/cli.js migration:run",
    "migrate:generate": "SET NODE_ENV=production&& ts-node ./node_modules/.bin/typeorm migration:generate -n",
    "migrate:prod": "NODE_ENV=production ts-node ./node_modules/.bin/typeorm migration:run"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.0",
    "@nestjs/common": "6.11.11",
    "@nestjs/core": "6.11.11",
    "@nestjs/jwt": "6.1.2",
    "@nestjs/microservices": "6.11.11",
    "@nestjs/passport": "6.2.0",
    "@nestjs/platform-express": "6.11.11",
    "@nestjs/platform-socket.io": "6.11.11",
    "@nestjs/typeorm": "6.3.3",
    "@nestjs/websockets": "6.11.11",
    "axios": "^0.19.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.2.0",
    "class-validator": "^0.9.1",
    "cookie": "^0.4.0",
    "cookie-parser": "^1.4.4",
    "custom-env": "^1.0.0",
    "dotenv": "^7.0.0",
    "joi": "^14.3.1",
    "moment": "^2.24.0",
    "node-geocoder": "^3.24.0",
    "nonce": "^1.0.4",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pg": "^7.9.0",
    "reflect-metadata": "^0.1.12",
    "request": "^2.88.2",
    "request-promise": "^4.2.4",
    "rimraf": "2.6.3",
    "rxjs": "6.4.0",
    "shopify-api-node": "^2.25.1",
    "soap": "^0.29.0",
    "stylelint-config-prettier": "^5.2.0",
    "stylelint-prettier": "^1.1.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typeorm": "^0.2.15"
  },
  "devDependencies": {
    "@nestjs/testing": "6.0.1",
    "@types/express": "4.16.1",
    "@types/jest": "24.0.11",
    "@types/joi": "^14.3.2",
    "@types/node": "11.11.4",
    "@types/supertest": "2.0.7",
    "jest": "24.5.0",
    "nodemon": "1.18.10",
    "prettier": "1.16.4",
    "supertest": "4.0.2",
    "ts-jest": "24.0.0",
    "ts-node": "8.6.2",
    "tsconfig-paths": "3.8.0",
    "tslint": "5.14.0",
    "typescript": "3.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
